@model IEnumerable<UGBWebApi.Models.Producao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IndiceDisponibilidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IndicePerfomance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IndiceQualidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resultado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QtdeProduzida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fechado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IndiceDisponibilidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IndicePerfomance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IndiceQualidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resultado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdeProduzida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fechado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
